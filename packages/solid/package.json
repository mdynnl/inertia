{
  "name": "@inertiajs/solid",
  "version": "0.0.1",
  "license": "MIT",
  "description": "The Solid adapter for Inertia.js",
  "contributors": [
    "Jonathan Reinink <jonathan@reinink.ca>",
    "Sebastian De Deyne <sebastiandedeyne@gmail.com>"
  ],
  "homepage": "https://inertiajs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiajs/inertia.git",
    "directory": "packages/solid"
  },
  "bugs": {
    "url": "https://github.com/inertiajs/inertia/issues"
  },
  "files": ["src", "dist"],
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "src/server.ts"
      ]
    }
  },
  "scripts": {
    "dev": "./build.js --watch",
    "__build": "npm run clean && ./build.js && tsc --emitDeclarationOnly",
    "build": "npm run clean && ./build.js",
    "clean": "rm -rf types && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "esbuild": "^0.16.13",
    "solid-js": "^1.7.5",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "solid-js": "^1.7.5"
  },
  "dependencies": {
    "@inertiajs/core": "1.0.6",
    "lodash.isequal": "^4.5.0"
  }
}
